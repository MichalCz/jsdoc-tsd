{
	"name": "@otris/jsdoc-tsd",
	"version": "0.8.2",
	"description": "JSDoc Template for generate typescript definition files from JSDoc comments",
	"main": "./src-out/core/publish.js",
	"scripts": {
		"compile": "tsc -p ./",
		"compile-watch": "npm run compile -- --watch",
		"compile-tests": "tsc -p ./test",
		"parse-example": "move /Y src-out\\publish.js src-out\\publish.backup.js && copy /Y src-out\\publish-example.js src-out\\publish.js && jsdoc -r exampleProject -t src-out && move /Y src-out\\publish.backup.js src-out\\publish.js",
		"test": "cross-env TS_NODE_PROJECT=test/tsconfig.json TS_NODE_FILES=true mocha --opts test/mocha.opts",
		"test-extension": "jsdoc -r exampleProject -t src-out"
	},
	"author": "Maximilian Wehrstedt",
	"license": "MIT",
	"devDependencies": {
		"@types/chai": "^4.0.4",
		"@types/mocha": "^5.2.7",
		"chai": "^4.1.2",
		"cross-env": "^5.2.0",
		"grunt": "^1.0.1",
		"grunt-jsdoc": "^2.1.1",
		"jsdoc3-parser": "^2.0.0",
		"mocha": "^6.1.4",
		"ts-node": "^8.3.0",
		"tslint": "^5.7.0"
	},
	"dependencies": {
		"@types/node": "^12.0.12",
		"@types/shelljs": "^0.8.5",
		"dts-dom": "^3.3.0",
		"node-version-compare": "^1.0.1",
		"shelljs": "^0.8.3",
		"typescript": "^3.5.2"
	},
	"peerDependencies": {
		"jsdoc": "^3.5.5"
	}
}
